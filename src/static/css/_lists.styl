.list-divider-slash
    margin 0
    padding 0

    li
        display inline-block

        + li:before
            content '/'
            padding 0 .3em 0 .1em
            color $lightgray

.text-divider-slash + .text-divider-slash:before
    content '/'
    padding 0 .3em 0 .1em
    color $lightgray
